import os
from xml.etree import ElementTree

file_name = '/Users/lmichaele/Desktop/ebay.xml'

dom = ElementTree.parse(file_name)

things = dom.findall('listing')

for c in things:

    sellerinfo = c.find('seller_info/seller_name').text
    shipinf = c.find('shipping_info').text
    print(sellerinfo,shipinf)
